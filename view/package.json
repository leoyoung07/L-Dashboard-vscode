{
  "name": "l-toolset-view",
  "description": "A Vue.js project",
  "version": "1.0.0",
  "author": "Leo Young <420760135@qq.com>",
  "license": "MIT",
  "scripts": {
    "build-todo-list": "parcel build ./src/entries/todo-list/entry.html --public-url . -d ../out/view/todo-list",
    "dev-todo-list": "parcel -p 8888 ./src/entries/todo-list/entry.html",
    "test": "jest",
    "lint": "eslint --ext .js,.vue src test/unit"
  },
  "dependencies": {
    "moment": "^2.19.4",
    "vue": "^2.5.11"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "testRegex": "(/tests/unit/specs/.*|\\.(test|spec))\\.(vue|js)$",
    "mapCoverage": true
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "cross-env": "^5.0.5",
    "node-sass": "^4.7.2",
    "vue-jest": "^1.4.0",
    "jest": "^22.0.4",
    "babel-jest": "^22.0.4",
    "eslint": "^4.14.0",
    "eslint-config-standard": "^7.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.0.1",
    "eslint-plugin-vue": "^4.2.0",
    "eslint-config-vue": "^2.0.2",
    "babel-eslint": "^8.2.1",
    "parcel-plugin-eslint": "^1.0.0",
    "parcel-bundler": "^1.3.1",
    "parcel-plugin-vue": "^1.4.0"
  }
}
