{
    "name": "l-dashboard",
    "displayName": "L-Dashboard",
    "description": "",
    "version": "0.0.1",
    "publisher": "LeoYoung",
    "engines": {
        "vscode": "^1.17.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.showDashboard",
        "onCommand:extension.startNginx",
        "onCommand:extension.stopNginx"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.showDashboard",
                "title": "Show Dashboard"
            },
            {
                "command": "extension.startNginx",
                "title": "Start Nginx"
            },
            {
                "command": "extension.stopNginx",
                "title": "Stop Nginx"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.2",
        "vscode": "^1.1.5",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "ws": "^3.2.0"
    }
}
